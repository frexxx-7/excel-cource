{
  "parser":"@babel/eslint-parser",
  "parserOptions": {
    "requireConfigFile":false
  },
  "env":{
    "es6":true,
    "browser":true,
    "node":true,
    "jest/globals": true 
  },
  "rules":{
    "semi":"off",
    "arrow-parens":"off",
    "comma-dangle":"off",
    "linebreak-style": "off",
    "require-jsdoc":"off",
    "operator-linebreak":"off"
  },
  "plugins": ["jest"],
  "extends": ["eslint:recommended", "google"]
}